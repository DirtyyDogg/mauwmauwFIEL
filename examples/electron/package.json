    "@theia/preview": "^0.8.0",
    "@theia/process": "^0.8.0",
    "@theia/python": "^0.8.0",
    "@theia/search-in-workspace": "^0.8.0",
    "@theia/task": "^0.8.0",
    "@theia/terminal": "^0.8.0",
    "@theia/textmate-grammars": "^0.8.0",
    "@theia/tslint": "^0.8.0",
    "@theia/typehierarchy": "^0.8.0",
