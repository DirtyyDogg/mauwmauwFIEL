    "@theia/filesystem": "^0.8.0",
    "@theia/getting-started": "^0.8.0",
    "@theia/git": "^0.8.0",
    "@theia/java": "^0.8.0",
    "@theia/java-debug": "^0.8.0",
    "@theia/json": "^0.8.0",
    "@theia/keymaps": "^0.8.0",
    "@theia/languages": "^0.8.0",
